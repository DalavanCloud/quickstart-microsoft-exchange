        "VPCID": {
            "Description": "ID of the VPC (e.g., vpc-0343606e)",
            "Type": "AWS::EC2::VPC::Id"
        },
        "Volume1Iops": {
            "Default": "1000",
            "Description": "Iops for the Exchange Data drive (Only used when volume type is io1)",
            "MaxValue": "20000",
            "MinValue": "100",
            "Type": "Number"
