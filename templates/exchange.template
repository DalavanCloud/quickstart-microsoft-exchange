            "MaxValue": "16000",
            "MinValue": "100",
            "Type": "Number"
        },
        "Volume1Type": {
            "AllowedValues": [
                "gp2",
                "io1"
            ],
            "Default": "gp2",
            "Description": "Volume type for the Exchange Data drive",
            "Type": "String"
        },
        "Volume2Iops": {
            "Default": "1000",
            "Description": "Iops for the Exchange drive (Only used when volume type is io1)",
            "MaxValue": "20000",
            "MinValue": "100",
            "Type": "Number"
